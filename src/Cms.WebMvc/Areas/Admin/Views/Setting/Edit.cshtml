@model Cms.Infrastructure.Persistence.Entities.Setting
@{
    Layout = "_AdminLayout";
    
}

<div class="container">
    <h4>Chỉnh sửa Cấu hình</h4>
    <form method="post">
        <input type="hidden" asp-for="Id">
        <div class="form-row">
            <div class="form-group col-md-4">
                <label class="tx-bold"  asp-for="Key">Cấu hình</label>
                <input asp-for="Key" class="form-control">
            </div>
            <div class="form-group col-md-4">
                <label class="tx-bold" asp-for="Value">Gía trị</label>
                <input asp-for="Value" class="form-control">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-2">
                <input type="submit" value="Submit" class="form-control btn btn-primary">
            </div>
                    
        </div>
        

    </form>
    <!-- /.row-->
</div>