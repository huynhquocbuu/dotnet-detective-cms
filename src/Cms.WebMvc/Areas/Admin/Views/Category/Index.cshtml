@model List<Cms.Application.Admin.Models.Post.CategoryDto>
@{
    Layout = "_AdminLayout";
    
}

<div class="container">
    <h4>Quản lý danh mục</h4>
    <div class="row">

        <div class="table-responsive">
            <table class="table">
                <thead>
                <tr>
                    <th scope="col"><a href="/Admin/Category/Add" class="text-primary"><i data-feather="plus-circle"></i></a></th>
                    <th scope="col">Title</th>
                    <th scope="col">Slug</th>
                </tr>
                </thead>

                <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td><a class="text-primary" href="/Admin/Category/Edit/@item.Id"><i data-feather="edit"></i></a> <a class="text-danger" href="/Admin/Category/Delete/@item.Id"> <i data-feather="delete"></i></a></td>
                        <td>@item.Title</td>
                        <td>@item.Slug</td>

                    </tr>
                }


                </tbody>

            </table>
        </div>

    </div>
    <!-- /.row-->
</div>