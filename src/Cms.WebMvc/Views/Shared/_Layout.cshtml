<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>@ViewData["Title"]</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="~/assets/arsha/img/favicon.png" rel="icon">
  <link href="~/assets/arsha/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css2?family=Roboto&family=Rubik Bubbles" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Jost:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Nosifer" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Agbalumo" rel="stylesheet">
  
  <!-- Vendor CSS Files -->
  <link href="~/assets/arsha/vendor/aos/aos.css" rel="stylesheet">
  <link href="~/assets/arsha/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="~/assets/arsha/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="~/assets/arsha/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="~/assets/arsha/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="~/assets/arsha/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="~/assets/arsha/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="~/assets/arsha/css/style.css" rel="stylesheet">
  <link href="~/assets/arsha/css/my-public-styles.css" rel="stylesheet">
  
   @await RenderSectionAsync("Styles", required: false)
</head>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top ">
    <partial name="Partials/Public/_Header" />
  </header><!-- End Header -->

  <!-- ======= Hero Section ======= -->
  <section id="hero" class="d-flex align-items-center">
    <partial name="Partials/Public/_Hero" />
  </section><!-- End Hero -->

  <main id="main">

    <!-- ======= Cliens Section ======= -->
    @* <section id="cliens" class="cliens section-bg"> *@
    @*   <partial name="Partials/Public/_Clients" /> *@
    @* </section> *@
    <!-- End Cliens Section -->

    <!-- ======= About Us Section ======= -->
    <section id="about" class="about">
      <partial name="Partials/Public/_AboutUs" />
    </section><!-- End About Us Section -->

    <!-- ======= Why Us Section ======= -->
    <section id="why-us" class="why-us section-bg">
      <partial name="Partials/Public/_WhyUs" />
    </section><!-- End Why Us Section -->

    <!-- ======= Skills Section ======= -->
    @* <section id="skills" class="skills"> *@
    @*   <partial name="Partials/Public/_Skills" /> *@
    @* </section> *@
    <!-- End Skills Section -->

    <!-- ======= Services Section ======= -->
    <section id="services" class="services section-bg">
      <partial name="Partials/Public/_Services" />
    </section><!-- End Services Section -->

    <!-- ======= Cta Section ======= -->
    @* <section id="cta" class="cta"> *@
    @*   <partial name="Partials/Public/_CallToAction" /> *@
    @* </section> *@
    <!-- End Cta Section -->

    <!-- ======= Portfolio Section ======= -->
  @* <section id="portfolio" class="portfolio"> *@
  @*   <partial name="Partials/Public/_Portfolio" /> *@
  @* </section> *@
  <!-- End Portfolio Section -->

  <!-- ======= Team Section ======= -->
  <!-- ======= Body Section ======= -->
    <section id="team" class="team section-bg">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>THÔNG TIN THÁM TỬ</h2>
          <p>Các thông tin trong lĩnh vực thám tử.</p>
        </div>

        <div class="row">
          @RenderBody()
        </div>
        

      </div>
    </section><!-- End Team Section -->

    <!-- ======= Pricing Section ======= -->
    @* <section id="pricing" class="pricing"> *@
    @*   <partial name="Partials/Public/_Pricing" /> *@
    @* </section> *@
    <!-- End Pricing Section -->

    <!-- ======= Frequently Asked Questions Section ======= -->
    <section id="faq" class="faq section-bg">
      <partial name="Partials/Public/_FAQ" />
    </section><!-- End Frequently Asked Questions Section -->

    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
      <partial name="Partials/Public/_Contact" />
    </section><!-- End Contact Section -->

  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  <footer id="footer">
    <partial name="Partials/Public/_Footer" />
  </footer><!-- End Footer -->

  <div id="preloader"></div>
  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="~/assets/arsha/vendor/aos/aos.js"></script>
  <script src="~/assets/arsha/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="~/assets/arsha/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="~/assets/arsha/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="~/assets/arsha/vendor/php-email-form/validate.js"></script>
  <script src="~/assets/arsha/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="~/assets/arsha/vendor/waypoints/noframework.waypoints.js"></script>

  <!-- Template Main JS File -->
  <script src="~/assets/arsha/js/main.js"></script>

  @await RenderSectionAsync("Scripts", required: false)
</body>

</html>